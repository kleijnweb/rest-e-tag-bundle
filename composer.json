{
  "name": "kleijnweb/rest-e-tag-bundle",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "John Kleijn",
      "email": "john@kleijnweb.nl",
      "homepage": "http://www.kleijnweb.nl"
    }
  ],
  "license": "LGPL-3.0",
  "description": "Caching and Concurrency Control for REST APIs using E-Tag Headers",
  "autoload": {
    "psr-4": {
      "KleijnWeb\\RestETagBundle\\": "src"
    }
  },
  "require": {
    "php": "^7.0",
    "psr/log": "^1.0",
    "symfony/dependency-injection": ">=2.8.7",
    "symfony/config": ">=2.8.7",
    "symfony/event-dispatcher": ">=2.8.7",
    "symfony/http-foundation": ">=2.8.7",
    "symfony/http-kernel": ">=2.8.7",
    "symfony/yaml": ">=2.8.7",
    "symfony/filesystem": ">=2.8.7",
    "psr/simple-cache": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.2",
    "phpunit/phpunit-mock-objects": "^3.1",
    "symfony/framework-bundle": ">=2.8.7",
    "symfony/dom-crawler": ">=2.8.7",
    "symfony/monolog-bundle": ">=2.8.7",
    "mikey179/vfsStream": "^1.5",
    "symfony/browser-kit": ">=2.8.7",
    "symfony/finder": ">=2.8.7",
    "symfony/form": ">=2.8.7",
    "symfony/routing": ">=2.8.7",
    "satooshi/php-coveralls": "<1.0",
    "symfony/cache": "dev-master"
  },
  "config": {
    "bin-dir": "bin"
  }
}
